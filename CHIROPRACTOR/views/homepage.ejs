<!doctype html>
<html lang="en">

<head>

  <link rel="stylesheet" href="style.css">
  <style>
    .bg-mynav {
      background-color: #2c3e50;
    }

    body {
      font-size: 1.25rem;
      background-color: #f6f8fa;
    }

    td {
      line-height: 3rem;
    }
  </style>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <!-- Bootstrap CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-+0n0xVW2eSR5OomGNYDnhzAbDsOXxcvSN1TPprVMTNDbiYZCxYbOOl7+AMvyTG2x" crossorigin="anonymous">

  <title>chiropractor</title>

</head>

<body>

  <nav class="navbar navbar-dark bg-mynav">
    <div class="container-fluid">
      <a class="navbar-brand" href="#">หมอหนวด</a>
    </div>
  </nav>

  <div class="container">
    <div class="d-flex bd-highlight mb-3">
      <div class="me-auto p-2 bd-highlight">
        <h2>CHIROPRACTOR
      </div>
      <div class="p-2 bd-highlight">
        <a href="../showchiropractor" class="btn btn-info">รายชื่อหมอนวด</a>
        
      </div>
    </div>

    <div>
      <div class="table-responsive">

        <form class="" action="/customer" method="post">

          <label for=>ใส่ชื่อท่านและเลือกบริการที่ท่านต้องการ</label><br>
          <!--<input type="text" name="" value="<%= Math.floor(Math.random()*99001)+1000 %>"><br>-->

          <label for=>Name:</label><br>
          <input type="text" name="name" value=" "><br>

          <label for=>Request:</label><br>
          <h5><select id="Request" name="Request">
            <option value="นวดคอ">นวดคอ</option>
            <option value="นวดบ่า">นวดบ่า</option>
            <option value="นวดไหล่">นวดไหล่</option>
            <option value="นวดหลัง">นวดหลัง</option>
            <option value="นวดฝ่าเท้า">นวดฝ่าเท้า</option>
            <option value="นวดแผนไทย">นวดแผนไทย</option>
            <option value="นวดกดจุด">นวดกดจุด</option>
          </select><br></h5>

          <input type="submit" class="btn btn-success btn-sm" name="button" id="btn" value="Add">
          <a class="btn btn-warning btn-sm" href="/">Reset Data</a><br>

        </form>
      </div>



      <tr>
        <th scope="row" colspan="5"><br>
          <h1>Path man</h1>
        </th>
      </tr>

      <table class="table">
      
        <thead>
          <tr>
            <th scope="col">Customer_id</th>
            <th scope="col">Name</th>
            <th scope="col">Require</th>
            <th scope="col">หมอหนวด</th>
          </tr>
      
          <form method="get">
            <% customer.forEach(function(item) { %>
              <tr>
                <td>
                  <%= item.Customer_id %>
                </td>
                <td>
                  <%= item.name %>
                </td>
                <td>
                  <%= item.Request %>
                </td>
                <% if(item.Request==="นวดคอ" ){ %>
                  <td>Somboon   Somjai  Somchai</td>
                  <td>
                    <a href="../delete/<%= item.path_id %>" class="btn btn-danger delete">Delete</a>
                  </td>
                  <% } else if(item.Request==="นวดบ่า" ){ %>
                    <td>Somboon   Somjai  Somchai</td>
                    <td>
                      <a href="../delete/<%= item.Customer_id %>" class="btn btn-danger delete">Delete</a>
                    </td>
                    <% } else if(item.Request==="นวดไหล่" ){ %>
                      <td>Somboon   Somjai  Somchai</td>
                      <td>
                        <a href="../delete/<%= item.Customer_id %>" class="btn btn-danger delete">Delete</a>
                      </td>
                      <% } else if(item.Request==="นวดหลัง" ){ %>
                        <td>Somboon   Somrak </td>
                        <td>
                          <a href="../delete/<%= item.Customer_id %>" class="btn btn-danger delete">Delete</a>
                        </td>
                        <% } else if(item.Request==="นวดฝ่าเท้า" ){ %>
                          <td>Somboon   Somporn   Somsri  Somsak  Somjai  Somrak  Somchai   Somsak</td>
                          <td>
                            <a href="../delete/<%= item.Customer_id %>" class="btn btn-danger delete">Delete</a>
                          </td>
                          <% } else if(item.Request==="นวดแผนไทย" ){ %>
                            <td>Somporn   Somrak  Somsak</td>
                            <td>
                              <a href="../delete/<%= item.Customer_id %>" class="btn btn-danger delete">Delete</a>
                            </td>
                            <% } else if(item.Request==="นวดกดจุด" ){ %>
                              <td>Somrak  Somjai  Somsri</td>
                              <td>
                                <a href="../delete/<%= item.Customer_id %>" class="btn btn-danger delete">Delete</a>
                              </td>
                    <% } %>
              </tr>
              <% }) %>
          </form>
        </thead>



        <tbody id="mytable">
          <tr>
            <th scope="row" colspan="5">

              <h4>Loading...</h4><br>

              <a class="btn btn-secondary btn-sm" href="../delete">
                <h3>Reset Data</h3>
                <a class="btn btn-secondary btn-sm" href="../show1">
                  <h3>1</h3>
              </a><br><br>

            </th>
          </tr>
        </tbody>
      </table>

      <div class="container">
        <h1 class="mt-5">62050185 Nanthawat Minjindakul</h1>
      </div>

    </div>
  </div>

  <!-- <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11.0.16/dist/sweetalert2.all.min.js"></script> -->
  <!-- <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-gtEjrD/SeCtmISkJkNUaaKMoLD0//ElJ19smozuHV6z3Iehds+3Ulb9Bn9Plx0x4" crossorigin="anonymous"></script> -->

</body>

</html>